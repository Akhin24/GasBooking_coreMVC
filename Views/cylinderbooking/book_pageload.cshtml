@model GasBooking_coreMVC.Models.Boookingclass
<form asp-action="book_click" class="form-horizondal" method="post">

    <div>
        <label asp-for="SelectedModeId">Mode</label>
        <select asp-for="SelectedModeId" asp-items="Model.ModelList" class="form-control">Select Mode</select>
    </div>

    <div>
        <label asp-for="SelectedCylinderId">Cylinder Type</label>
        <select asp-for="SelectedCylinderId" asp-items="Model.CylinderList"class="form-control">Select Cylinder</select>
    </div>
    <div>
        <label>Consumer Number</label>
        <input asp-for="consumbernumber" class="form-control" id="t1"/>
    </div>
    <div>
        <input type="submit" value="Book" class="btn btn-success" />
    </div>
</form>
<div>
    @Html.ActionLink("Back To Home page", "customerHome", "login")
</div>
@{
    if (@TempData["bmsg"] != null)
    {
        <script>
            alert('@TempData["bmsg"]');
            document.getElementById("t1").value = "";
        </script>
    }

}