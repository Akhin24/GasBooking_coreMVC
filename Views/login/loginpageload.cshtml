@model GasBooking_coreMVC.Models.Logincls
<form asp-action="loginclick" class="form-horizondal" method="post">
    <div class="form-group">
        <label class="control-label">Username</label>
        <input asp-for="username" class="form-control" id="t1" />
    </div>
    <div class="form-group">
        <label class="control-label">Password</label>
        <input asp-for="password" class="form-control" id="t2" />
    </div>

    <div class="form-group">
        <input type="submit" value="login" class="btn btn-success" />
    </div>
</form>
@{
    if (@TempData["logmsg"] != null)
    {
        <script>
            alert('@TempData["logmsg"]');
            document.getElementById("t1").value = "";
            document.getElementById("t2").value = "";
        </script>
    }

}