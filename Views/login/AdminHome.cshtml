@model GasBooking_coreMVC.Models.Cylinderinsert
<form asp-action="cylinderadd" class="form-horizondal" method="post" >

   <div class="form-group">
        <label class="control-label">Cylinder Type</label>
        <select asp-for="cyname" asp-items="ViewBag.cylist"></select>
    </div>

    <div class="form-group">
        <label class="control-label">Cylinder stock</label>
        <input asp-for="stock" class="form-control" id="c2" />
    </div>

    <div class="form-group">
        <label class="control-label">Filled</label>
        <input asp-for="filled" class="form-control" id="c3" />
    </div>

    <div class="form-group">
        <label class="control-label">Not filled</label>
        <input asp-for="notfilled" class="form-control" id="c4" />
    </div>
    <div class="form-group">
        <label class="control-label">Cylinder Price</label>
        <input asp-for="price" class="form-control" id="c5" />
    </div>

    <div class="form-group">
        <input type="submit" value="Insert" class="btn btn-success" />
    </div>


</form>
@{
    if (@TempData["clymsg"] != null)
    {
        <script>
            alert('@TempData["clymsg"]');
            document.getElementById("c1").value = "";
            document.getElementById("c2").value = "";
            document.getElementById("c3").value = "";
            document.getElementById("c4").value = "";
            document.getElementById("c5").value = "";
        </script>
    }

}